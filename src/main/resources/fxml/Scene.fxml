<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="576.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.synapticon.buckeusbaccessory.FXMLController">
    <children>
      <VBox prefHeight="576.0" prefWidth="1024.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#onClose" text="Close" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
              <items>
                  <Button fx:id="switchToUSBAccessoryModeButton" mnemonicParsing="false" onAction="#onSwitchToUSBAccessoryMode" text="Switch to USB accessory mode" />
                  <TextField fx:id="codeTextField" onAction="#handleCodeTextChanged" promptText="to verify against" text="qwerty" />
                  <ComboBox fx:id="sendIntervalComboBox" prefWidth="150.0" promptText="Send Interval" />
                  <Label text="ms" />
              </items>
            </ToolBar>
            <ToolBar prefHeight="40.0" prefWidth="200.0">
               <items>
                  <Label text="States:">
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <ToggleButton mnemonicParsing="false" onAction="#onSmartphoneConnected" selected="true" text="SMARTPHONE CONNECTED" />
                  <ToggleButton mnemonicParsing="false" onAction="#onHeadlightToggle" text="HEADLIGHT" />
                  <ToggleButton mnemonicParsing="false" onAction="#onCameraToggle" text="CAMERA" />
                  <ToggleButton mnemonicParsing="false" onAction="#onLeftTurnSignalToggle" text="LEFT TURN SIGNAL" />
                  <ToggleButton mnemonicParsing="false" onAction="#onRightTurnSignalToggle" text="RIGHT TURN SIGNAL" />
                  <ToggleButton mnemonicParsing="false" onAction="#onGasThrottleIdleStateToggle" selected="true" text="GAS THROTTLE IDLE STATE" />
               </items>
            </ToolBar>
            <HBox prefHeight="120.0" prefWidth="1004.0" spacing="10.0">
               <children>
                  <Slider fx:id="speedSlider" blockIncrement="1.0" majorTickUnit="10.0" max="50.0" minorTickCount="1" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" />
                  <Slider fx:id="batteryPowerSlider" blockIncrement="1.0" majorTickUnit="512.0" max="512.0" min="-512.0" minorTickCount="1" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
                  <Slider fx:id="batteryStateOfChargeSlider" blockIncrement="1.0" majorTickUnit="10.0" minorTickCount="1" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="80.0" />
                  <Slider fx:id="remainingDistanceSlider" blockIncrement="1.0" majorTickUnit="50.0" max="255.0" minorTickCount="1" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" value="150.0" />
                  <Slider fx:id="totalDistanceSlider" blockIncrement="1.0" majorTickUnit="16300.0" max="65535.0" minorTickCount="1" orientation="VERTICAL" showTickLabels="true" showTickMarks="true" value="950.0" />
               </children>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </HBox>
            <TextArea fx:id="logTextArea" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextArea>
         </children>
      </VBox>
    </children>
</AnchorPane>
